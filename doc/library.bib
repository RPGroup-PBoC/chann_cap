file = {:C$\backslash$:/Users/talei/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Perfeito et al. - 2011 - Nonlinear Fitness Landscape of a Molecular Pathway.pdf:pdf},
isbn = {0011-3891},
