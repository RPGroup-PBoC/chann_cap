@article{Marshall2015,
abstract = {The cell represents a highly organized state of living matter, in which numerous geometrical parameters are under dynamic regulation in order to match the form of a cell with its function. Cells appear capable of regulating not only the total quantity of their internal organelles but also the size and number of those organelles. The regulation of three parameters—size, number, and total quantity—can, in principle, be accomplished by regulating the production or growth of organelles, their degradation or disassembly, and their partitioning among daughter cells during division. Any or all of these steps could, in principle, be under regulation. But whether organelle assembly or disassembly is regulated by number or size raises interesting questions about how the cell would recognize how many copies of an organelle it has or how big they are. Expected final online publication date for the Annual Review of Biophysics Volume 45 is May 6, 2016. Please see http://www.annualreviews.org/catalog/pubdates.aspx for r...},
author = {Marshall, Wallace F.},
journal = {Annual Review of Biophysics},
language = {en},
month = {jul},
publisher = {Annual Reviews 4139 El Camino Way, PO Box 10139, Palo Alto, California 94303-0139, USA},
title = {{Cell Geometry: How Cells Count and Measure Size}},
year = {2015}
}
