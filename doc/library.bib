file = {:C$\backslash$:/Users/talei/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Brewster et al. - 2014 - The transcription factor titration effect dictates level of gene expression.pdf:pdf},
issn = {1097-4172},
