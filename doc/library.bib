issn = {1095-9130},
journal = {Methods (San Diego, Calif.)},
keywords = {Bacteria,Binding Sites,Eukaryota,Gene Expression Regulation,Models, Genetic,Promoter Regions, Genetic,Protein Binding,RNA, Messenger,RNA, Messenger: genetics,RNA, Messenger: metabolism,Single-Cell Analysis,Stochastic Processes,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism,Transcription, Genetic},
