month = {sep},
number = {37},
pages = {34508--20},
pmid = {12107163},
title = {{Global allostery model of hemoglobin. Modulation of O(2) affinity, cooperativity, and Bohr effect by heterotropic allosteric effectors.}},
volume = {277},
year = {2002}
}
