keywords = {Algorithms,Biological,Cluster Analysis,Databases,Diabetes Mellitus,Gene Expression Profiling,Gene Expression Regulation,Gene Regulatory Networks,Genetic,Humans,Male,Metabolic Networks and Pathways,Metabolome,Models,Muscle,Oligonucleotide Array Sequence Analysis,Regulatory Elements,Skeletal,Transcription,Transcriptional,Type 2,Type 2: genetics,Type 2: metabolism},
