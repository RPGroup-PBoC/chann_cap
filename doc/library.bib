journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Allosteric Regulation,Amino Acid Sequence,Aspartate Carbamoyltransferase,Aspartate Carbamoyltransferase: chemistry,Aspartate Carbamoyltransferase: metabolism,Binding Sites,Crystallography,Escherichia coli,Escherichia coli: enzymology,Models,Molecular,Protein Conformation,Static Electricity,X-Ray},
