keywords = {Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Gene Dosage,Gene Expression,Gene Expression Regulation, Bacterial,Models, Genetic,Plasmids,Polymerase Chain Reaction,Promoter Regions, Genetic,Thermodynamics,Transcription Factors,Transcription Factors: metabolism,Transcription, Genetic},
