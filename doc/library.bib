doi = {10.1016/j.jmb.2013.03.013},
file = {:C$\backslash$:/Users/talei/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Marzen, Garcia, Phillips - 2013 - Statistical mechanics of Monod-Wyman-Changeux (MWC) models.pdf:pdf},
